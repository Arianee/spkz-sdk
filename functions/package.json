    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.6.1",
    "jest": "^24.8.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0"
  },
  "devDependencies": {
    "tslint": "^5.12.0",
    "typescript": "^3.8.0",
    "firebase-functions-test": "^0.2.0"
